{
  "name": "woof-frontend",
  "version": "2.0.0",
  "description": "Woof - Dog Social Network (Frontend)",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "npm run build && \"C:/Program Files/Amazon/AWSCLIV2/aws.exe\" s3 sync dist/ s3://woofapp.fi/ --delete --exclude '.DS_Store'",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "npx playwright test",
    "test:e2e:auth": "npx playwright test e2e/auth.spec.ts",
    "test:e2e:dog": "npx playwright test e2e/dog.spec.ts",
    "test:e2e:headed": "npx playwright test --headed"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.3.1",
    "@types/node": "^22.10.5",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "happy-dom": "^20.6.0",
    "jsdom": "^28.0.0",
    "typescript": "^5.9.3",
    "vite": "^6.0.6",
    "vitest": "^4.0.18"
  },
  "author": "Tommi Kivisaari",
  "license": "UNLICENSED",
  "dependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "amazon-cognito-identity-js": "^6.3.16",
    "svelte": "^5.53.2"
  }
}
